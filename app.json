{
  "expo": {
    "name": "Mobile Timing",
    "slug": "RDTiming",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splashicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#00ac65"
    },
    "scheme": "rdtiming",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.runsignup.rdtiming"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#00ac65"
      },
      "package": "com.runsignup.rdtiming"
    },
    "extra": {
      "bugsnag": {
        "apiKey": "5866860e489df33e3dccd903bae0b14d"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "@bugsnag/expo/hooks/post-publish.js",
          "config": {}
        }
      ]
    }
  }
}